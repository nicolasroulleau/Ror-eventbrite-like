<h1>Nouvel événement</h1>

<%= form_for @event do |f| %>
<p><%= f.label :date %><br>
   <%= f.date_field :date %></p>
<p><%= f.label :description %><br>
   <%= f.text_field :description %></p>
<p><%= f.label :place %><br>
   <%= f.text_field :place %></p>
  <%= f.submit "Valider" %>
<% end %>

</br>
<div><%= link_to"Retour",events_path %></div>


